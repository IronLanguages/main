;;; Let binding
;;;

(defun foo2 (x)
   (princ "x ")
   (print x)
   (let* ((x "let x")
          (y 7))
      ;; Test shadow binding local names.
      (print "in let* ...")
      (print x) ;had runtime type null for x in print???????
      (princ "y: ")
      (print y)
      (set x 5)
      (princ "let x ")
      (print x))
   (princ "arg x ")
   (print x)
   (princ "blah ")
   (print blah)
   ;; Test shadow binding global names.
   (let* ((blah "let blah"))
      (print blah)
      (set blah "bill")
      (print blah))
   (princ "blah: ")
   (print blah)
   (set blah 17))

(print (foo2 11))
(print blah)


